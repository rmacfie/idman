{
  "name": "idman",
  "version": "0.0.0",
  "description": "Identity management service",
  "author": "Robert Macfie <robert@macfie.se>",
  "private": true,
  "main": "src/server.js",
  "scripts": {
    "clean": "del ./src/**/*.js ./test/**/*.js ./src/**/*.js.map ./test/**/*.js.map",
    "build": "tsc",
    "prestart": "tsc",
    "start": "node src/server.js",
    "pretest": "tsc",
    "test": "mocha 'test/**/*.test.js'",
    "watch:server": "nodemon --delay 0.5",
    "watch:ts": "tsc -w",
    "watch:mocha": "mocha -w 'test/**/*.test.js'",
    "watch": "npm-run-all -p -l watch:ts watch:server",
    "testwatch": "npm-run-all -p watch:ts watch:mocha"
  },
  "dependencies": {
    "base32-decode": "^0.1.0",
    "base32-encode": "^0.1.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.9",
    "pg": "^6.1.0",
    "superagent": "^2.3.0",
    "validator": "^6.1.0",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/chai": "^3.4.34",
    "@types/express": "^4.0.33",
    "@types/jsonwebtoken": "^7.1.33",
    "@types/mocha": "^2.2.32",
    "@types/pg": "^6.1.34",
    "@types/superagent": "^2.0.35",
    "@types/supertest-as-promised": "^2.0.32",
    "@types/validator": "^5.7.34",
    "@types/winston": "0.0.29",
    "chai": "^3.5.0",
    "del-cli": "^0.2.0",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.1",
    "typescript": "^2.0.6"
  }
}
